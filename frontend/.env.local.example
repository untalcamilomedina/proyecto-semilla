# Archivo de ejemplo para desarrollo local
# Copia este archivo como .env.local y configura tus valores

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000/api/v1
NEXT_PUBLIC_API_TIMEOUT=30000

# Application Configuration
NEXT_PUBLIC_APP_NAME=Proyecto Semilla
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=development

# Development Configuration
NEXT_PUBLIC_ENABLE_DEVTOOLS=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Feature Flags
NEXT_PUBLIC_ENABLE_COLLABORATION=true
NEXT_PUBLIC_ENABLE_MARKETPLACE=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# Demo Configuration
NEXT_PUBLIC_DEMO_EMAIL=demo@demo-company.com
NEXT_PUBLIC_DEMO_PASSWORD=demo123
NEXT_PUBLIC_DEFAULT_TENANT_ID=8aa99184-4011-4cfc-b2cb-82b64f10d72b