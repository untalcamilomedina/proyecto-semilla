# Archivo de ejemplo para desarrollo local
# Copia este archivo como .env.local y configura tus valores

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:7777
NEXT_PUBLIC_API_TIMEOUT=30000

# Application Configuration
NEXT_PUBLIC_APP_NAME=Proyecto Semilla
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=development

# Development Configuration
NEXT_PUBLIC_ENABLE_DEVTOOLS=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Feature Flags
NEXT_PUBLIC_ENABLE_COLLABORATION=true
NEXT_PUBLIC_ENABLE_MARKETPLACE=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false

# =============================================================================
# SECURITY NOTE: Credentials removed from frontend
# =============================================================================
# User creation is now handled through the setup wizard
# No credentials should be stored in NEXT_PUBLIC_ variables as they are
# exposed to the client browser
# =============================================================================

# Default tenant ID (optional, for development only)
NEXT_PUBLIC_DEFAULT_TENANT_ID=00000000-0000-0000-0000-000000000001