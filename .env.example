# Variables de entorno para desarrollo
# Copie este archivo como .env y ajuste los valores según necesite

# Base de datos
DB_PASSWORD=your_secure_password_here
DB_HOST=db
DB_PORT=5432
DB_NAME=proyecto_semilla
DB_USER=admin

# Backend
JWT_SECRET=your_jwt_secret_key_at_least_64_characters_long_for_security
CORS_ORIGINS=http://localhost:7701,http://localhost:7777

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:7777

# Configuración adicional
DEBUG=false
LOG_LEVEL=INFO

# Contraseñas de seed data (cambiar en producción)
SEED_ADMIN_PASSWORD=admin123
SEED_DEMO_PASSWORD=demo123

# Cookie Security Configuration
COOKIE_SECURE=false
COOKIE_DOMAIN=
COOKIE_SAME_SITE=lax

# Redis
REDIS_URL=redis://redis:6379
